{
  "info": {
    "name": "Reeder API Test",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "1. Login",
      "request": {
        "method": "POST",
        "url": "{{base_url}}/api/greader.php/accounts/ClientLogin",
        "header": {
          "Content-Type": "application/x-www-form-urlencoded"
        },
        "body": {
          "mode": "urlencoded",
          "urlencoded": [
            {"key": "Email", "value": "user"},
            {"key": "Passwd", "value": "pass"},
            {"key": "accountType", "value": "HOSTED_OR_GOOGLE"},
            {"key": "client", "value": "Reeder"},
            {"key": "output", "value": "json"},
            {"key": "service", "value": "reader"}
          ]
        }
      }
    },
    {
      "name": "2. Get User Info",
      "request": {
        "method": "GET",
        "url": "{{base_url}}/api/greader.php/reader/api/0/user-info",
        "query": [
          {"key": "output", "value": "json"}
        ]
      }
    },
    {
      "name": "3. Get Subscription List",
      "request": {
        "method": "GET",
        "url": "{{base_url}}/api/greader.php/reader/api/0/subscription/list",
        "query": [
          {"key": "output", "value": "json"}
        ]
      }
    },
    {
      "name": "4. Get Unread Item IDs",
      "request": {
        "method": "GET",
        "url": "{{base_url}}/api/greader.php/reader/api/0/stream/items/ids",
        "query": [
          {"key": "n", "value": "10000"},
          {"key": "output", "value": "json"},
          {"key": "s", "value": "user/-/state/com.google/reading-list"},
          {"key": "xt", "value": "user/-/state/com.google/read"}
        ]
      }
    },
    {
      "name": "5. Get Starred Item IDs",
      "request": {
        "method": "GET",
        "url": "{{base_url}}/api/greader.php/reader/api/0/stream/items/ids",
        "query": [
          {"key": "n", "value": "10000"},
          {"key": "output", "value": "json"},
          {"key": "s", "value": "user/-/state/com.google/starred"}
        ]
      }
    },
    {
      "name": "6. Get Item Content (ID: 213)",
      "request": {
        "method": "POST",
        "url": "{{base_url}}/api/greader.php/reader/api/0/stream/items/contents",
        "header": {
          "Content-Type": "application/x-www-form-urlencoded"
        },
        "body": {
          "mode": "urlencoded",
          "urlencoded": [
            {"key": "T", "value": "{\"token\":\"12345/abcdef\"}"},
            {"key": "i", "value": "tag:google.com,2005:reader/item/213"},
            {"key": "output", "value": "json"}
          ]
        }
      }
    },
    {
      "name": "7. Get Item Content (ID: 262)",
      "request": {
        "method": "POST",
        "url": "{{base_url}}/api/greader.php/reader/api/0/stream/items/contents",
        "header": {
          "Content-Type": "application/x-www-form-urlencoded"
        },
        "body": {
          "mode": "urlencoded",
          "urlencoded": [
            {"key": "T", "value": "{\"token\":\"12345/abcdef\"}"},
            {"key": "i", "value": "tag:google.com,2005:reader/item/262"},
            {"key": "output", "value": "json"}
          ]
        }
      }
    },
    {
      "name": "8. Get Item Content (ID: 38)",
      "request": {
        "method": "POST",
        "url": "{{base_url}}/api/greader.php/reader/api/0/stream/items/contents",
        "header": {
          "Content-Type": "application/x-www-form-urlencoded"
        },
        "body": {
          "mode": "urlencoded",
          "urlencoded": [
            {"key": "T", "value": "{\"token\":\"12345/abcdef\"}"},
            {"key": "i", "value": "tag:google.com,2005:reader/item/38"},
            {"key": "output", "value": "json"}
          ]
        }
      }
    },
    {
      "name": "9. Get Read Items Since Timestamp",
      "request": {
        "method": "GET",
        "url": "{{base_url}}/api/greader.php/reader/api/0/stream/items/ids",
        "query": [
          {"key": "n", "value": "10000"},
          {"key": "ot", "value": "1735900620"},
          {"key": "output", "value": "json"},
          {"key": "s", "value": "user/-/state/com.google/read"}
        ]
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:4000"
    }
  ]
} 